<h1>Viewing Party Lite</h1>

<% unless session[:user_id] %>
  <%= button_to 'Create a New User', "/register", method: :get %>
  <%= link_to 'Log In', "/login", method: :get %>
  <% else %>
    <%= link_to 'Log Out', '/logout', method: :delete %>
<% end  %>

<h1>Existing Users:</h1>
<% @users.each do |user| %>
<ol>
  <%= link_to "#{user.email}'s Dashboard", "/users/#{user.id}" %>
</ol>
<% end %>
